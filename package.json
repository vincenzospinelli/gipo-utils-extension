{
  "name": "gipo-utils-extension",
  "version": "1.1.3",
  "description": "**GipoUtils** Ã¨ un'estensione per Chrome che include due strumenti interattivi e configurabili:",
  "main": "content.js",
  "type": "module",
  "scripts": {
    "build:css": "npx tailwindcss -i ./assets/styles/styles.css -o ./assets/styles/tailwind.css --minify",
    "build:vite": "vite build && vite build --config vite.content.config.js",
    "build": "rimraf dist && mkdir dist && npm run build:css && npm run build:vite && node scripts/copy-to-dist.js",
    "package:zip": "rimraf package && mkdir -p package && zip -r package/package.zip dist/*",
    "prepublish:version": "node scripts/update-version-from-tag.js",
    "refresh:token": "node scripts/refresh-token.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "express": "^5.1.0",
    "googleapis": "^153.0.0",
    "open": "^10.2.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "fs-extra": "^11.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
